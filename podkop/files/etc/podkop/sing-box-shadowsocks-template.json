{
    "log": {
      "level": "warn"
    },
    "inbounds": [
        {
        "type": "tproxy",
        "listen": "::",
        "listen_port": 1602,
        "sniff": false
        }
    ],
    "outbounds": [
        {
          "type": "shadowsocks",
          "server": "$HOST",
          "server_port": "$PORT",
          "method": "$METHOD",
          "password": "$PASS"
        }
      ],
    "route": {
      "auto_detect_interface": true
    }
}
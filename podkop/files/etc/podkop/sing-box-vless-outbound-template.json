{
  "outbounds": [
    {
      "type": "vless",
      "server": "$HOST",
      "server_port": "$PORT",
      "uuid": "$UUID",
      "flow": "xtls-rprx-vision",
      "tls": {
        "enabled": true,
        "insecure": false,
        "server_name": "$FAKE_SERVER",
        "utls": {
          "enabled": true,
          "fingerprint": "chrome"
        },
        "reality": {
          "enabled": true,
          "public_key": "$PUBLIC_KEY",
          "short_id": "$SHORT_ID"
        }
      },
      "tag": "$TAG"
    }
  ]
}